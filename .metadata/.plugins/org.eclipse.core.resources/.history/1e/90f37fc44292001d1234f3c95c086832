package ex02이차원배열;

public class Ex04세로로넣기 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [][] array = new int [5][5];
		
		array[0][0] = 21 ; 
		array[0][1] = 26 ; 
		array[0][2] = 31 ; 
		array[0][3] = 36 ; 
		array[0][4] = 41 ; 
		
		
		array[1][0] = 22 ; 
		array[1][1] = 27 ; 
		array[1][2] = 32 ; 
		array[1][3] = 37 ; 
		array[1][4] = 42 ; 
		
		System.out.println(array[0][0]);
		
				
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
