package Study;

public class Ex03_1부터100까지더하고빼기 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//1-2+3-4+5-6+...+99-100
		//그리고 답 -50
		
		int i = 0;
		while ( i <= 10) {
			if (i % 2 == 1) {
				
				System.out.print("-" + i);
			}
			i++;
			
		}
		
		
		
		
		
		
		
		
		
		
		
	}

}
