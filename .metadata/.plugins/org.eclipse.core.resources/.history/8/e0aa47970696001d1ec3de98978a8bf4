package ex02객체지향기초;

public class Bankbook {

	// 속성
	//잔액
	int money ; 
	
	// 기능
	// 입금하다(-)
	public void deposit(int Money1) {
		money = Money1 + money;
	}
	

	
	// 출금하다(-)
	
	public void withdraw(int Money2) {
		money = money - Money2;
	}
	
	
	
	// 잔액 보기
	
	public void showMoney() {
		System.out.println(money);
	}
	
	
	
	
	
	
	
	
	
}
