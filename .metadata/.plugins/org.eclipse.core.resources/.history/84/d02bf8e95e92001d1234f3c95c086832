package ex01;

public class Ex01구구단보충 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int [][] array = new int [9][9];
		
		int num = 0;
		
		
		for( int i = 2 ; i <= 9 ; i++) {
			num = i * 1 ;
			System.out.print( i  + "*" + "1" + " = " + num +" ");
		}
		System.out.println(" ");
		for( int i = 2 ; i <= 9 ; i++) {
			num = i * 1 ;
			System.out.print( i  + "*" + "1" + " = " + num +" ");
		}
		System.out.println(" ");
		for( int i = 2 ; i <= 9 ; i++) {
			num = i * 1 ;
			System.out.print( i  + "*" + "1" + " = " + num +" ");
		}
		System.out.println(" ");
		
		
		
		
		
		
//		2 * 1 = 2 / 
//		2 * 2 = 4
//		2 * 3 = 6
//		2 * 4 = 8
//		2 * 5 = 10
