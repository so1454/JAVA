package ex01_0110;

public class Ex12배열실습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] starCount = {3,4,4,2,1};
		
		System.out.print(starCount[0] + " : "); //3을 출력시켜야함
		
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.println(" ");
		
		System.out.print(starCount[1] + " : "); //4를 출력시켜야함
		
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		
		
		
		
		
		
		
		
		
		
	}

}
