package ex02객체지향기초;

public class BankbookMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Bankbook B1 = new Bankbook();
		B1.money = 1000;
		System.out.println("잔액 : "+  B1.money);
		
		int in = 5000;
		B1.deposit(in);
		System.out.println("입금 후 잔액: " + B1.money);
		
		
	}

}
