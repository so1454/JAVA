package ex02객체지향기초;

public class BankbookMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Bankbook B1 = new Bankbook();
		B1.money = 10000;
		B1.showMoney();
		
		int in = 5000;
//		System.out.println("입금 금액 : "+ in);
		B1.deposit(in);
		System.out.println("입금 후 잔액: " + B1.money);
		
		int out = 5000;
//		System.out.println("출금 금액 : "+ out);
		B1.withdraw(out);
		System.out.println("출금 후 잔액: " + B1.money);
		
		B1.showMoney();
		
		
		
		
	}

}
