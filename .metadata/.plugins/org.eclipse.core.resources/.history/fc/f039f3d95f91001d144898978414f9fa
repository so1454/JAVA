package 이차원배열;

public class Ex02_1부터25까지넣기 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[][] array = new int[5][5];

		array[0][0] = 1;
		array[0][1] = 2;
		array[0][2] = 3;
		array[0][3] = 4;
		array[0][4] = 5;

		array[1][0] = 1;
		array[1][1] = 2;
		array[1][2] = 3;
		array[1][3] = 4;
		array[1][4] = 5;

		array[2][0] = 1;
		array[2][1] = 2;
		array[2][2] = 3;
		array[2][3] = 4;
		array[2][4] = 5;

		System.out.println(array[0][0] + " ");

	}

}
